/* pages/personalCenter/personalCenter.wxss */
.bottom-buttons {
    display: flex;
    justify-content: space-around;
    width: 100%;
    position: fixed;
    bottom: 0;
    left: 0;
    background-color: #fff;
    padding: 20rpx 0;
    z-index: 999;
    padding-bottom: constant(safe-area-inset-bottom); /* 兼容 iOS < 11.2 */
    padding-bottom: env(safe-area-inset-bottom);     /* 兼容 iOS >= 11.2 */
  }
 
  
  
  
  